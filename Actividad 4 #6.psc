Proceso sin_titulo
	Definir var1, var2, var3 Como Entero;
	Definir nombre, materia Como Caracter;
	
	Mostrar "Escriba  el nombre aquí por favor";
	Leer nombre;
	Mostrar "Escriba la materia";
	Leer materia;
	Mostrar "Escriba los tres resultados aquí:";
	Leer var1, var2, var3;
	
	Definir valorx Como Real;
	valorx=(var1*.33)+(var2*.33)+(var3*.34);
	
	Imprimir "El estudiante: " nombre " en " materia " tiene un resultado de " valorx;
	
FinProceso
