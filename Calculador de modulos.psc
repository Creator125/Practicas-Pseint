Proceso calculador_de_modulos
	Definir var1, var2, resultado Como Entero;
	Escribir "Escriba el primer valor para el modulo";
	Leer var1;
	Escribir "Escriba el segundo nuemro para el modulo";
	Leer var2;
	
	resultado= var1 mod var2;
	Imprimir "El modulo de tu operación es: ", resultado ;
FinProceso
